#, kde-format
msgid "Cannot start initramfs."
msgstr "initramfs आरभुं न शक्नोति।"

#: helper.cpp:171
#, kde-format
msgid "Cannot start mkinitcpio."
msgstr "mkinitcpio आरभुं न शक्नोति।"

#: helper.cpp:180
#, kde-format
msgid "Initramfs failed to run."
msgstr "इनिट्रम्फ्स् धावितुं असफलः अभवत् ।"

#: helper.cpp:190
#, kde-format
